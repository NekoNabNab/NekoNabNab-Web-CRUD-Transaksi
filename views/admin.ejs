    <div class="p-4 p-md-5 mb-4 row " id="TablePage">
      <h3 class="subJudul mb-2">Data Input From User</h3>
        <table class="border" cellpadding="8">
            <ul>
                <tr style="background-color:#5FA8A0; color: #d6fffb;">
                    <th class="border fw-normal"> User Name </th>
                    <th class="border fw-normal"> Email </th>
                    <th class="border fw-normal"> Status </th>
                    <th class="border fw-normal"> Password </th>
                    <th class="border fw-normal"> Edit </th>
                    <th class="border fw-normal"> Update </th>
                    <th class="border fw-normal"> Delete </th>
                </tr>
                <%for(const s of buyer) { %> 
                    <tr>
                        <th class="border"> <%= s.nama %> </th>
                        <th class="border"> <%= s.email %> </th>
                        <th class="border"> <%= s.status %> </th>
                        <th class="border"> <%= s.password %> </th>
                        <th class="border"><a class="text-dark text-decoration-none" href="/edit/<%= s.id %>">Edit</a></th>
                        <th class="border"><a class="text-dark text-decoration-none" href="/update/<%= s.id %>">Update</a></th>
                        <th class="border"><a class="text-dark text-decoration-none" href="/delete/<%= s.id %>">Delete</a></th>
                    </tr>
                <% } %>
            </ul>
        </table>
    </div>
